{
  "name": "eslint-plugin-no-react",
  "version": "0.0.0-development",
  "description": "Eslint plugin to forbid React import and usage from global declaration",
  "main": "./dist/index.js",
  "files": [
    "dist/**"
    ],
  "author": "denkrasnov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/denkrasnov/eslint-plugin-no-react"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --colors",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "format": "eslint --ignore-path .gitignore --ext .js,.ts --fix .",
    "check:types": "tsc -p tsconfig.json --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.13",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "mocha": "^9.0.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
