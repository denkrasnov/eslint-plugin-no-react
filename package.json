{
  "name": "eslint-plugin-no-react-import",
  "version": "0.0.1",
  "description": "eslint plugin to lint react import",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --colors",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "format": "eslint --ignore-path .gitignore --ext .js,.ts --fix .",
    "check:types": "tsc -p tsconfig.json --noEmit"
  },
  "author": "denkrasnov",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^7.2.13",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.0.5",
    "mocha": "^9.0.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
